{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ImageSettings": {
    "MaxBytes": 6000000,
    "AcceptedFileTypes": [ ".jpg", ".jpeg", ".png", ".JPG", ".JPEG", ".PNG" ]
  },
  "ConnectionStrings": {
    "pathHbCs": "Server=<SERVER_GOES_HERE>;Initial Catalog=pathology-hb-db-uat;Persist Security Info=False;User ID=pathhb;Password=<PASSWORD_GOES_HERE>;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "StorageAccount": {
    "StorageAccountNameOption": "<STORAGE_ACCOUNT_NAME_GOES_HERE>",
    "StorageAccountKeyOption": "<ACCOUNT_KEY_OPTION_GOES_HERE>",
    "FullSizeContainerNameOption": "fullsize",
    "ThumbnailContainerNameOption": "thumbnails"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "Domain": "<AZURE_DOMAIN_GOES_HERE>",
    "TenantId": "<AZURE_TENANT_ID_GOES_HERE>",
    "ClientId": "<AZURE_CLIENT_ID_GOES_HERE>"
  }
}
